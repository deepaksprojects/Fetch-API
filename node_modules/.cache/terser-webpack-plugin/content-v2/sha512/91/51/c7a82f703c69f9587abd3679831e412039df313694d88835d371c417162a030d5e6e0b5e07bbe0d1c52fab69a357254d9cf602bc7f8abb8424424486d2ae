 // function\r\n  const handlePrev = () => {\r\n    const page = currentPage - 1;\r\n    setCurrentPage(page);\r\n  };\r\n  const handleNext = () => {\r\n    const page = currentPage + 1;\r\n    setCurrentPage(page);\r\n  };\r\n  const handleDelete = (index) => {\r\n    const newItem = user.filter((curEle) => {\r\n      return curEle.id != index;\r\n    });\r\n    setUser(newItem);\r\n  };\r\n  return (\r\n    <>\r\n      <div>\r\n        <div className=\"news-div-2\">\r\n          {currentPosts.map((curEle) => {\r\n            return (\r\n              <div>\r\n                <div className=\"img-div-2\">\r\n                  <img\r\n                    src=\"\\images\\close.svg\"\r\n                    alt=\"\"\r\n                    onClick={() => handleDelete(curEle.id)}\r\n                  />\r\n                </div>\r\n                <div className=\"news-c-div-2\" key={curEle.id}>\r\n                  <h1>{curEle.title}</h1>\r\n                  <p>{curEle.body}</p>\r\n                </div>\r\n                \r\n              </div>\r\n            );\r\n          })}\r\n        </div>\r\n\r\n        <Pagination\r\n          currentPage={currentPage}\r\n          handlePrev={handlePrev}\r\n          handleNext={handleNext}\r\n          pageNumber={pageNumber}\r\n          paginate={paginate}\r\n        />\r\n      </div>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default Newsverticle;\r\n","import React, { useState } from \"react\";\r\nimport \"./style.css\";\r\nimport News from \"./news\";\r\nimport Form from \"./form\";\r\nimport Newsverticle from \"./newsverticle\";\r\n\r\nconst Navbar = () => {\r\n  const [view, setView] = useState(\"horizontal\");\r\n  const [showModel, setShowModel] = useState(\"close\");\r\n\r\n\r\n  const handleClick = (e) => {\r\n    e.preventDefault();\r\n\r\n    switch (showModel) {\r\n      case \"open\":\r\n        setShowModel(\"close\");\r\n        break;\r\n      case \"close\":\r\n        setShowModel(\"open\");\r\n        break;\r\n      default:\r\n        setShowModel(\"close\");\r\n        break;\r\n    }\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <div className=\"main-div\">\r\n        <section className=\"section-div \">\r\n          <div className=\"first-s-div\">\r\n            Hi reader.\r\n            <img src=\"\" alt=\"\" />\r\n            <span className=\"f-s-d-s\">Here's Your News!</span>\r\n          </div>\r\n\r\n          <div className=\"second-s-div\">\r\n            <h3>View Toggel</h3>\r\n            <button\r\n              className=\"s-s-d-button\"\r\n              onClick={() => setView(\"horizontal\")}\r\n            >\r\n              <img src=\"\\images\\horizontal_split.svg\" alt=\"\" />\r\n            </button>\r\n            <button\r\n              className=\"s-s-d-button\"\r\n              onClick={() => setView(\"verticle\")}\r\n            >\r\n              <img src=\"\\images\\vertical_split.svg\" alt=\"\" />\r\n            </button>\r\n          </div>\r\n\r\n          <div className=\"third-s-div\">\r\n            Have a Feedback?\r\n            <button className=\"t-s-d-button\" onClick={handleClick}>\r\n              <span>We're Listening!</span>\r\n            </button>\r\n          </div>\r\n\r\n          <Form showModel={showModel} handleClick={handleClick} />\r\n        </section>\r\n\r\n        {view === \"verticle\" ? <Newsverticle /> : <News />}\r\n      </div>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default Navbar;\r\n","import News from './component/news';\nimport Navbar from './component/navbar';\n\nfunction App() {\n  return (\n    <>\n    <Navbar />\n    </>\n  );\n}\n\nexport default App;\n","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport './index.css';\nimport App from './App';\n\nReactDOM.render(\n  <React.StrictMode>\n    <App />\n  </React.StrictMode>,\n  document.getElementById('root')\n);\n\n\n","// extracted by mini-css-extract-plugin"]}}